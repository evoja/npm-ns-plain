    "watch": "tsc -p test --watch --out-dir out",
    "test": "npm run clean && npm run build-tests && npm run just-test",
    "just-test": "nodeunit out/test/tests"
  },
  "//devDependencies": {
    "//": "Testing purposes",
    "nodeunit": "Test runner"
  },

  
  "devDependencies": {
    "typescript": "3.7.3",
    "nodeunit": "0.11.3",
    "@types/nodeunit": "0.0.30"
  }
