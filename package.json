  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": ["lib"],

  "scripts": {
    "mypublish": "npm test && npm publish --access public",

    "clean": "rm -rf lib && rm -rf out",
    "build": "tsc -p src --outDir lib",
